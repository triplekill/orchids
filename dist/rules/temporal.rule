/*

 rule for testing temporal analyzer

*/

rule temporal {
  state init {
    $ctx = "svc_" + .syslog.prog;
    temporal($ctx1);

    $ctx = "host_" + str_from_ipv4(.udp.src_addr);
    temporal($ctx2);

    $ctx = "syslog_" + str_from_ipv4(.udp.src_addr) + "_" + .syslog.prog;
    /* $ctx = "syslog-" + .syslog.host + "-" + .syslog.prog; */
    temporal( $ctx );
  }
}
