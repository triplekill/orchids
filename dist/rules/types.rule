/*
** rule for testing constant types
*/


rule types
{
  state init {
    $a = 123;
    $b = "hello.world";
    $c = _CTIME( 1234567890 );
    $d = _CTIME( "Sat Feb 14 00:31:30 2009" );
    $e = _IPV4( "www.lsv.ens-cachan.fr" );
    $f = _TIMEVAL( 1234567890, 1234567890 );
    $g = _TIMEVAL( "Sat Feb 14 00:31:30 2009", 1234567890 );
    $h = _COUNTER( 12345 );
    $i = _REGEX( "^abc(de|fgh)*klm$" );
  }

  state end {
    print( "end" );
  }
}
